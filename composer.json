{
    "name": "jun/php-sentry-example",
    "description": "A PHP project integrating Sentry for error tracking with asynchronous HTTP requests using Guzzle.",
    "version": "0.0.1",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jun\\PhpSentryExample\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Jun\\PhpSentryExample\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Junseok Shin",
            "email": "sjsage522@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4 <9.0",
        "sentry/sentry": "^4.7",
        "guzzlehttp/guzzle": "^7.8",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5"
    },
    "scripts": {
        "test": "phpunit --testdox"
    }
}
